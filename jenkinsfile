            }
        }
        stage('Gouri') {
            steps {
                echo 'hello earth'
                
            }
        }
        stage('Manu') {
            steps {
                echo 'Hello sun'
            }
        }
        stage('Viji') {
            steps {
                echo 'hello moon'
            }
        }
    }
}

